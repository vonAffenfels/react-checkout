    },
    "homepage": "https://github.com/paolomagic/react-checkout#readme",
    "peerDependencies": {
        "@apollo/client": "^3.5.8",
        "graphql": "^16.4.0",
        "graphql-tag": "^2.12.6",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
