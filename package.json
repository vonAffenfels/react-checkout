        "@babel/preset-react": "^7.16.7",
        "@headlessui/react": "^1.7.13",
        "@heroicons/react": "^1.0.6",
        "@stripe/react-stripe-js": "^1.8.0-0",
        "@stripe/stripe-js": "^1.29.0",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.2",
        "autoprefixer": "^9.8.8",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.13",
